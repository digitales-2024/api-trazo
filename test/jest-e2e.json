{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@login/login/(.*)": "<rootDir>/../libs/login/src/$1",
    "@login/login": "<rootDir>/../libs/login/src",
    "@prisma/prisma/(.*)": "<rootDir>/../libs/prisma/src/$1",
    "@prisma/prisma": "<rootDir>/../libs/prisma/src",
    "@clients/clients/(.*)": "<rootDir>/../libs/clients/src/$1",
    "@clients/clients": "<rootDir>/../libs/clients/src",
    "@design-projects/design-projects/(.*)": "<rootDir>/../libs/design-projects/src/$1",
    "@design-projects/design-projects": "<rootDir>/../libs/design-projects/src",
    "@business/business/(.*)": "<rootDir>/../libs/business/src/$1",
    "@business/business": "<rootDir>/../libs/business/src",
    "@sunat-change/sunat-change/(.*)": "<rootDir>/../libs/sunat-change/src/$1",
    "@sunat-change/sunat-change": "<rootDir>/../libs/sunat-change/src",
    "@project-execution/project-execution/(.*)": "<rootDir>/../libs/project-execution/src/$1",
    "@project-execution/project-execution": "<rootDir>/../libs/project-execution/src"
  }
}